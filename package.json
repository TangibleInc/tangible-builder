    "gulp-terser": "^1.2.0",
    "gulp-watch": "eliot-akira/gulp-watch#custom",
    "highlight.js": "^10.1.1",
    "inquirer": "^7.2.0",
    "markdown-it": "^11.0.0",
    "mime": "^2.4.6",
    "minimist": "^1.2.5",
    "prettier": "^2.0.5",
    "serve-handler": "^6.1.3",
    "source-map-support": "^0.5.19",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "typescript": "^3.9.5",

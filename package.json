{
  "name": "@tangible/builder",
  "version": "1.6.6",
  "description": "Tangible Builder: Develop, build, beautify, schema, docs, test, serve",
  "main": "index.js",
  "bin": {
    "tgb": "./run"
  },
  "scripts": {
    "build": "cd plugins/schema-processor && tsc -p .",
    "release": "npm publish --access public",
    "release:schema": "cd plugins/schema-processor && npm publish --access public",
    "test-dev": "cd test && ../run dev",
    "test-lint": "cd test && ../run lint",
    "test-beautify": "cd test && ../run beautify",
    "test-schema": "cd test && ../run schema",
    "test-serve": "cd test && ../run serve",
    "test-build": "cd test && ../run build"
  },
  "dependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-proposal-do-expressions": "^7.10.1",
    "@babel/plugin-proposal-export-default-from": "^7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.3",
    "@babel/plugin-proposal-optional-chaining": "^7.10.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.10.1",
    "@babel/plugin-proposal-throw-expressions": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@tangible/schema-processor": "1",
    "@wordpress/browserslist-config": "^2.7.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babelify": "^10.0.0",
    "chalk": "^4.1.0",
    "ejs": "^3.1.3",
    "event-stream": "^4.0.1",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-babel": "^8.0.0",
    "gulp-bro": "^2.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",

    "markdown-it": "^12.0.4",
    "mime": "^2.5.2",
    "minimist": "^1.2.5",
    "prettier": "^2.2.1",
    "serve-handler": "^6.1.3",
    "source-map-support": "^0.5.19",
    "stylelint": "^13.12.0",
    "stylelint-config-recommended": "^4.0.0",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "typescript": "^4.2.3",
